* DELETE COLUMN
  @code sql
  ALTER TABLE "<TABLE>"
  DROP COLUMN "<COLUMN>";
  @end

* ALTER COLUMN
  @code sql
  ALTER TABLE "<TABLE>"
  ADD "<COLUMN>" uuid;
  @end

* ALTER COLUMN
  @code sql
  ALTER TABLE "<TABLE>"
  ALTER COLUMN "<COLUMN>" TYPE numeric(40,30) using amount::numeric(40,30);
  @end
